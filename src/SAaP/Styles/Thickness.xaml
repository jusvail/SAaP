    <Thickness x:Key="NavigationViewHeaderMargin">56,34,0,0</Thickness>
    <Thickness x:Key="NavigationViewPageContentMargin">56,24,56,0</Thickness>

    <Thickness x:Key="MenuBarContentMargin">36,24,36,0</Thickness>

    <Thickness x:Key="SettingsPageHyperlinkButtonMargin">-12,4,0,0</Thickness>
    <Thickness x:Key="SettingsPageGridElementMargin">4,4,0,0</Thickness>

    <Thickness x:Key="StandardButtonMargin">5,5,0,5</Thickness>
    <Thickness x:Key="DefaultButtonFlyOutMargin">-6,-6,-6,-6</Thickness>

    <CornerRadius x:Key="DefaultThickness">4</CornerRadius>
    <Thickness x:Key="DefaultBorderThickness">.5</Thickness>
    <Thickness x:Key="DefaultGridBorderMargin">-.5,-.5,0,0</Thickness>
    <Thickness x:Key="DefaultButtonMargin">0 5 5 5</Thickness>
    <Thickness x:Key="BottomOnlyMargin">0,0,0,5</Thickness>
    <Thickness x:Key="TopOnlyMargin">-1,5,0,0</Thickness>
    <Thickness x:Key="DefaultGridBorderPadding">5</Thickness>
    <Thickness x:Key="DefaultGridThickness">5</Thickness>
    <Thickness x:Key="DefaultRadioButtonThickness">5,0,-50,0</Thickness>
    <Thickness x:Key="DefaultTextBoxThickness">0,1,2,1</Thickness>
    <Thickness x:Key="SmallHeaderTextBoxThickness">-5 8 4 2</Thickness>
    <Thickness x:Key="MainContentTextBoxThickness">-5 2 4 5</Thickness>
    <Thickness x:Key="MainGridThickness">5</Thickness>
    <Thickness x:Key="SecondaryGridThickness">2</Thickness>

    <Thickness x:Key="ArrangeLeftGridMargin">-5,2,0,2</Thickness>

    <SolidColorBrush x:Key="DefaultBorderBrush">LightGray</SolidColorBrush>

    <Style x:Key="DefaultGridWithBorderStyle" TargetType="Grid">
        <Setter Property="CornerRadius" Value ="{StaticResource DefaultThickness}"></Setter>
        <Setter Property="BorderThickness" Value ="{StaticResource DefaultBorderThickness}" ></Setter>
        <Setter Property="BorderBrush" Value ="{StaticResource DefaultBorderBrush}"></Setter>
    </Style>

    <Style x:Key="GdStyle" TargetType="Grid">
        <Setter Property="Margin" Value="{StaticResource DefaultGridBorderMargin}"></Setter>
        <Setter Property="Padding" Value="{StaticResource DefaultGridBorderPadding}"></Setter>
        <Setter Property="BorderBrush" Value="{StaticResource DefaultBorderBrush}"></Setter>
        <Setter Property="BorderThickness" Value="{StaticResource DefaultBorderThickness}"></Setter>
        <Setter Property="Background" Value="{ThemeResource LayerFillColorDefaultBrush}"></Setter>
    </Style>

    <x:Double x:Key="TimePickerSelectorThemeMinWidth">40</x:Double>
    <Thickness x:Key="TimePickerFirstHostThemeMargin">100,0,0,0</Thickness>
    <Thickness x:Key="TimePickerThirdHostThemeMargin">0,0,0,0</Thickness>

    <Thickness x:Key="TradeListRowDefaultThickness">-16,0,-12,0</Thickness>

